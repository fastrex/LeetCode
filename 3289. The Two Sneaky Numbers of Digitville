class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> result;
        std::unordered_set<int> sett;
        for (auto& n : nums)
        {
            auto it = sett.find(n);
            if (it != sett.end())
            {
                result.push_back(n);
                continue;
            }
            sett.insert(n);
        }
        return result;
    }
};
